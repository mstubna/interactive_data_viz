<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title %></title>    
    <%= stylesheet_link_tag "normalize", "all" %>
    <%= javascript_include_tag "jquery-2.1.0", "d3", "d3.tip" %>
    <% unless current_page.data.js.nil? %>
      <% current_page.data.js.each do |js| %>
        <%= javascript_include_tag js %>
      <% end %>
    <% end %>
  </head>
  
  <body class="<%= page_classes %>">
    <nav><a href='/'><div id='logo'></div></a></nav>
    <h1><%= current_page.data.title %></h1>
    <a href="index.html">Go back to the main page</a></br>
    <a href="meetup_notes.html#<%= current_page.data.meetup_note_link %>">More information about this visualization.</a>
    
    <%= yield %>
    
  </body>
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-44149839-1', 'mountaintrackapps.com');
    ga('send', 'pageview');
  </script>
</html>